    function _bwa_index {
        _arguments \
            "*: :_files"

    }

    function _bwa_mem {
        _arguments \
            "*: :_files"

    }

    function _bwa_aln {
        _arguments \
            "*: :_files"

    }

    function _bwa_samse {
        _arguments \
            "*: :_files"

    }

    function _bwa_sampe {
        _arguments \
            "*: :_files"

    }

    function _bwa_bwasw {
        _arguments \
            "*: :_files"

    }


function _bwa {
    local line state

    function _commands {
        local -a commands
        commands=(
            'index:Index database sequences in the FASTA format'
            'mem:Align 70bp-1Mbp query sequences with the BWA-MEM algorithm'
            'aln:Find the SA coordinates of the input reads'
            'samse:Generate alignments in the SAM format given single-end reads'
            'sampe:Generate alignments in the SAM format given paired-end reads'
            'bwasw:Align query sequences in the in.fq file'
        )
        _describe 'command' commands
    }
 

    _arguments -C \
        ': :->cmd' \
        '*:: :->subcmd'

    case $state in
    (cmd)
        _commands
        ;;
    (subcmd)
        case $line[1] in
        (index)
            _bwa_index
            ;;

        (mem)
            _bwa_mem
            ;;

        (aln)
            _bwa_aln
            ;;

        (samse)
            _bwa_samse
            ;;

        (sampe)
            _bwa_sampe
            ;;

        (bwasw)
            _bwa_bwasw
            ;;

        esac
        ;;
     esac

}

