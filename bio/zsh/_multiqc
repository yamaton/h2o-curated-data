#compdef _multiqc multiqc

# Generated with h2o 0.1.18


function _multiqc {
    local line state

    function _commands {
        local -a commands
        commands=(
        )
        _describe 'command' commands
    }
 

    _arguments -C \
        '(-f --force)'{-f,--force}'[Overwrite any existing reports]' \
        '(-d --dirs)'{-d,--dirs}'[Prepend directory to sample names]' \
        '(-dd --dirs-depth)'{-dd,--dirs-depth}'[Prepend \[INT\] directories to sample names. Negative number to take from start of path.]' \
        '(-s --fullnames)'{-s,--fullnames}'[Do not clean the sample names (leave as full file name)]' \
        '(-i --title)'{-i,--title}'[Report title. Printed as page header, used for filename if not otherwise specified.]' \
        '(-b --comment)'{-b,--comment}'[Custom comment, will be printed at the top of the report.]' \
        '(-n --filename)'{-n,--filename}'[Report filename. Use '\''stdout'\'' to print to standard out.]' \
        '(-o --outdir)'{-o,--outdir}'[Create report in the specified output directory.]' \
        '(-t --template)'{-t,--template}'[Report template to use.]' \
        '--tag[Use only modules which tagged with this keyword, eg. RNA]' \
        '(--view-tags --view_tags)'{--view-tags,--view_tags}'[View the available tags and which modules they load]' \
        '(-x --ignore)'{-x,--ignore}'[Ignore analysis files (glob expression)]' \
        '--ignore-samples[Ignore sample names (glob expression)]' \
        '--ignore-symlinks[Ignore symlinked directories and files]' \
        '--fn_as_s_name[Use the log filename as the sample name]' \
        '--replace-names[Path to TSV file to rename sample names during report generation]' \
        '--sample-names[Path to TSV file containing alternative sample names for renaming buttons in the report]' \
        '--sample-filters[Path to TSV file containing show/hide patterns for the report]' \
        '(-l --file-list)'{-l,--file-list}'[Supply a file containing a list of file paths to be searched, one per row]' \
        '(-e --exclude)'{-e,--exclude}'[Do not use this module. Can specify multiple times.]' \
        '(-m --module)'{-m,--module}'[Use only this module. Can specify multiple times.]' \
        '--data-dir[Force the parsed data directory to be created.]' \
        '--no-data-dir[Prevent the parsed data directory from being created.]' \
        '(-k --data-format)'{-k,--data-format}'[Output parsed data in a different format. Default: tsv]' \
        '(-z --zip-data-dir)'{-z,--zip-data-dir}'[Compress the data directory.]' \
        '(-p --export)'{-p,--export}'[Export plots as static images in addition to the report]' \
        '(-fp --flat)'{-fp,--flat}'[Use only flat plots (static images)]' \
        '(-ip --interactive)'{-ip,--interactive}'[Use only interactive plots (HighCharts Javascript)]' \
        '--lint[Use strict linting (validation) to help code development]' \
        '--pdf[Creates PDF report with '\''simple'\'' template. Requires Pandoc to be installed.]' \
        '--no-megaqc-upload[Don'\''t upload generated report to MegaQC, even if MegaQC options are found]' \
        '(-c --config)'{-c,--config}'[Specific config file to load, after those in MultiQC dir / home dir / working dir.]' \
        '(--cl-config --cl_config)'{--cl-config,--cl_config}'[Specify MultiQC config YAML on the command line]' \
        '(-v --verbose)'{-v,--verbose}'[Increase output verbosity.]' \
        '(-q --quiet)'{-q,--quiet}'[Only show log warnings]' \
        '--profile-runtime[Add analysis of how long MultiQC takes to run to the report]' \
        '--no-ansi[Disable coloured log output]' \
        '--version[Show the version and exit.]' \
        '(-h --help)'{-h,--help}'[Show this message and exit.]' \
        '1: :_commands' \
        '*: :_files'

    case $state in
    (cmd)
        _commands
        ;;
    (subcmd)
        case $line[1] in
        esac
        ;;
     esac

}

