# Generated with h2o 0.1.18


function _subsample_bam {
    local line state

    function _commands {
        local -a commands
        commands=(
        )
        _describe 'command' commands
    }
 

    _arguments -C \
        '(-h --help)'{-h,--help}'[show this help message and exit]' \
        '(-o --output_prefix)'{-o,--output_prefix}'[Output prefix (default: sub_sampled)]' \
        '(-p --profile)'{-p,--profile}'[Stride in genomic coordinates for depth profile. (default: 1000)]' \
        '(-O --orientation)'{-O,--orientation}'[Sample only forward or reverse reads. (default: None)]' \
        '(-t --threads)'{-t,--threads}'[Number of threads to use. (default: -1)]' \
        '(-q --quality)'{-q,--quality}'[Filter reads by mean qscore. (default: None)]' \
        '(-a --accuracy)'{-a,--accuracy}'[Filter reads by accuracy. (default: None)]' \
        '(-c --coverage)'{-c,--coverage}'[Filter reads by coverage (what fraction of the read aligns). (default: None)]' \
        '(-l --length)'{-l,--length}'[Filter reads by read length. (default: None)]' \
        '--any_fail[Exit with an error if any region has insufficient coverage. (default: False)]' \
        '--all_fail[Exit with an error if all regions have insufficient coverage. (default: False)]' \
        '(-x --patience)'{-x,--patience}'[Maximum iterations with no change in median coverage before aborting. (default: 5)]' \
        '(-s --stride)'{-s,--stride}'[Stride in genomic coordinates when searching for new reads. Smaller can lead to more compact pileup. (default: 1000)]' \
        '(-P --proportional)'{-P,--proportional}'[Activate proportional sampling, thus keeping depth variations of the pileup. (default: False)]' \
        '(-S --seed)'{-S,--seed}'[Random seed for proportional downsampling of reads. (default: None)]' \
        '1: :_commands' \
        '*: :_files'

    case $state in
    (cmd)
        _commands
        ;;
    (subcmd)
        case $line[1] in
        esac
        ;;
     esac

}

