function _tabix {
    local line state

    function _commands {
        local -a commands
        commands=(
        )
        _describe 'command' commands
    }
 

    _arguments -C \
        '(-0 --zero-based)'{-0,--zero-based}'[Specify that the position in the data file is 0-based (e.g. UCSC files) rather than 1-based.]' \
        '(-b --begin)'{-b,--begin}'[Column of start chromosomal position. \[4\]]' \
        '(-c --comment)'{-c,--comment}'[Skip lines started with character CHAR. \[#\]]' \
        '(-C --csi)'{-C,--csi}'[Produce CSI format index instead of classical tabix or BAI style indices.]' \
        '(-e --end)'{-e,--end}'[Column of end chromosomal position. The end column can be the same as the start column. \[5\]]' \
        '(-f --force)'{-f,--force}'[Force to overwrite the index file if it is present.]' \
        '(-m --min-shift)'{-m,--min-shift}'[set minimal interval size for CSI indices to 2^INT \[14\]]' \
        '(-p --preset)'{-p,--preset}'[Input format for indexing. Valid values are: gff, bed, sam, vcf. This option should not be applied together with any of -s, -b, -e, -c and -0; it is not used for data retrieval because this setting is stored in the index file. \[gff\]]' \
        '(-s --sequence)'{-s,--sequence}'[Column of sequence name. Option -s, -b, -e, -S, -c and -0 are all stored in the index file and thus not used in data retrieval. \[1\]]' \
        '(-S --skip-lines)'{-S,--skip-lines}'[Skip first INT lines in the data file. \[0\]]' \
        '(-h --print-header)'{-h,--print-header}'[Print also the header/meta lines.]' \
        '(-H --only-header)'{-H,--only-header}'[Print only the header/meta lines.]' \
        '(-l --list-chroms)'{-l,--list-chroms}'[List the sequence names stored in the index file.]' \
        '(-r --reheader)'{-r,--reheader}'[Replace the header with the content of FILE]:file:_files' \
        '(-R --regions)'{-R,--regions}'[Restrict to regions listed in the FILE. The FILE can be BED file (requires .bed, .bed.gz, .bed.bgz file name extension) or a TAB-delimited file with CHROM, POS, and, optionally, POS_TO columns, where positions are 1-based and inclusive. When this option is in use, the input file may not be sorted.]:file:_files' \
        '(-T --targets)'{-T,--targets}'[Similar to -R but the entire input will be read sequentially and regions not listed in FILE will be skipped.]:file:_files' \
        '-D[Do not download the index file before opening it. Valid for remote files only.]' \
        '--cache[Set the BGZF block cache size to INT megabytes. \[10\]]' \
        '--separate-regions[This option can be used when multiple regions are supplied in the command line and the user needs to quickly see which file records belong to which region. For this, a line with the name of the region, preceded by the file specific comment symbol, is inserted in the output before its corresponding group of records.]' \
        '--verbosity[Set verbosity of logging messages printed to stderr. The default is 3, which turns on error and warning messages; 2 reduces warning messages; 1 prints only error messages and 0 is mostly silent. Values higher than 3 produce additional informational and debugging messages.]' \
        '1: :_commands' \
        '*: :_files'

    case $state in
    (cmd)
        _commands
        ;;
    (subcmd)
        case $line[1] in
        esac
        ;;
     esac

}

