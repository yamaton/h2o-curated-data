# Auto-generated with h2o

_robot()
{
    local i=1 cmd cur word_list
    cur="${COMP_WORDS[COMP_CWORD]}"

    # take the last word that's NOT starting with -
    while [[ ( "$i" < "$COMP_CWORD" ) ]]; do
        local s="${COMP_WORDS[i]}"
        case "$s" in
          -*) ;;
          *)
            cmd="$s"
            ;;
        esac
        (( i++ ))
    done

    case "$cmd" in
      *)
          word_list="  --rpa -F --extension -N --name -D --doc -M --metadata -G --settag -t --test --task -s --suite -i --include -e --exclude -R --rerunfailed -S --rerunfailedsuites --runemptysuite --skip --skiponfailure -n --noncritical -c --critical -v --variable -V --variablefile -d --outputdir -o --output -l --log -r --report -x --xunit --xunitskipnoncritical -b --debugfile -T --timestampoutputs --splitlog --logtitle --reporttitle --reportbackground --maxerrorlines -L --loglevel --suitestatlevel --tagstatinclude --tagstatexclude --tagstatcombine --tagdoc --tagstatlink --expandkeywords --removekeywords --flattenkeywords --listener --nostatusrc --dryrun -X --exitonfailure --exitonerror --skipteardownonexit --randomize --prerunmodifier --prerebotmodifier --console -. --dotted --quiet -W --consolewidth -C --consolecolors -K --consolemarkers -P --pythonpath -A --argumentfile -h -? --help --version"
          COMPREPLY=( $(compgen -W "${word_list}" -- "${cur}") )
          ;;
    esac

}

## -o bashdefault and -o default are fallback
complete -o bashdefault -o default -F _robot robot
