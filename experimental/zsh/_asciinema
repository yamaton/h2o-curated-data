#compdef _asciinema asciinema

# Auto-generated with h2o

    function _asciinema_rec {
        _arguments \
            {-h,--help}'[show this help message and exit]' \
            '--stdin[enable stdin recording, disabled by default]' \
            '--append[append to existing recording]' \
            '--raw[save only raw stdout output]' \
            '--overwrite[overwrite the file if it already exists]' \
            {-c,--command}'[command to record, defaults to $SHELL]' \
            {-e,--env}'[list of environment variables to capture, defaults to SHELL,TERM]' \
            {-t,--title}'[title of the asciicast]' \
            {-i,--idle-time-limit}'[limit recorded idle time to given number of seconds]' \
            {-y,--yes}'[answer "yes" to all prompts (e.g. upload confirmation)]' \
            {-q,--quiet}'[be quiet, suppress all notices/warnings (implies -y)]' \
            "*: :_files"

    }

    function _asciinema_play {
        _arguments \
            {-h,--help}'[show this help message and exit]' \
            {-i,--}'[idle-time-limit IDLE_TIME_LIMIT limit idle time during playback to given number of seconds]' \
            {-s,--speed}'[playback speedup (can be fractional)]' \
            "*: :_files"

    }

    function _asciinema_cat {
        _arguments \
            {-h,--help}'[show this help message and exit]' \
            "*: :_files"

    }

    function _asciinema_upload {
        _arguments \
            {-h,--help}'[show this help message and exit]' \
            "*: :_files"

    }

    function _asciinema_auth {
        _arguments \
            {-h,--help}'[show this help message and exit]' \
            "*: :_files"

    }


function _asciinema {
    local line state

    function _commands {
        local -a commands
        commands=(
            'rec:Record terminal session'
            'play:Replay terminal session'
            'cat:Print full output of terminal session'
            'upload:Upload locally saved terminal session to asciinema.org'
            'auth:Manage recordings on asciinema.org account'
        )
        _describe 'command' commands
    }
 

    _arguments -C \
        {-h,--help}'[show this help message and exit]' \
        '--version[show program'\''s version number and exit]' \
        ': :->cmd' \
        '*:: :->subcmd'

    case $state in
    (cmd)
        _commands
        ;;
    (subcmd)
        case $line[1] in
        (rec)
            _asciinema_rec
            ;;

        (play)
            _asciinema_play
            ;;

        (cat)
            _asciinema_cat
            ;;

        (upload)
            _asciinema_upload
            ;;

        (auth)
            _asciinema_auth
            ;;

        esac
        ;;
     esac

}

