function _procs {
    local line state

    function _commands {
        local -a commands
        commands=(
        )
        _describe 'command' commands
    }
 

    _arguments -C \
        {-a,--and}'[AND logic for multi-keyword]' \
        {-o,--or}'[OR logic for multi-keyword]' \
        {-d,--nand}'[NAND logic for multi-keyword]' \
        {-r,--nor}'[NOR logic for multi-keyword]' \
        {-l,--list}'[Show list of kind]' \
        '--thread[Show thread]' \
        {-t,--tree}'[Tree view]' \
        {-w,--watch}'[Watch mode with default interval (1s)]' \
        {-W,--watch-interval}'[Watch mode with custom interval]' \
        {-i,--insert}'[Insert column to slot]' \
        '--only[Specified column only]' \
        '--sorta[Sort column by ascending]' \
        '--sortd[Sort column by descending]' \
        {-c,--color}'[Color mode]' \
        '--theme[Theme mode]' \
        {-p,--pager}'[Pager mode]' \
        '--interval[Interval to calculate throughput]' \
        '--config[Generate configuration sample file]' \
        '--completion[Generate shell completion file]' \
        '--completion-out[Generate shell completion file and write to stdout]' \
        '--no-header[Suppress header]' \
        {-h,--help}'[Print help information]' \
        {-V,--version}'[Print version information]' \
        '*: :_files'

    case $state in
    (cmd)
        _commands
        ;;
    (subcmd)
        case $line[1] in
        esac
        ;;
     esac

}

