# Auto-generated with h2o

_cmake()
{
    local i=1 cmd cur word_list
    cur="${COMP_WORDS[COMP_CWORD]}"

    # take the last word that's NOT starting with -
    while [[ ( "$i" < "$COMP_CWORD" ) ]]; do
        local s="${COMP_WORDS[i]}"
        case "$s" in
          -*) ;;
          *)
            cmd="$s"
            ;;
        esac
        (( i++ ))
    done

    case "$cmd" in
      *)
          word_list="  -S -B -C -D -U -G -T -A --toolchain --install-prefix -Wno-dev -Wdev -Werror -Wno-error -Wdeprecated -Wno-deprecated -Werror -Wno-error -L -N --graphviz --system-information --log-level --log-context --debug-trycompile --debug-output --debug-find --trace --trace-expand --trace-format --trace-source --trace-redirect --warn-uninitialized --warn-unused-vars --no-warn-unused-cli --check-system-vars --profiling-output --profiling-format --preset --list-presets --build --preset --list-presets --parallel -j --target -t --config --clean-first --use-stderr --verbose -v -- --install --config --component --default-directory-permissions --prefix --strip -v --verbose --zstd --files-from --format --mtime --help-full --help-manual --help-manual-list --help-command --help-command-list --help-commands --help-module --help-module-list --help-modules --help-policy --help-policy-list --help-policies --help-property --help-property-list --help-properties --help-variable --help-variable-list --help-variables"
          COMPREPLY=( $(compgen -W "${word_list}" -- "${cur}") )
          ;;
    esac

}

## -o bashdefault and -o default are fallback
complete -o bashdefault -o default -F _cmake cmake
