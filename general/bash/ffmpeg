# Auto-generated with h2o

_ffmpeg()
{
    local i=1 cmd cur word_list
    cur="${COMP_WORDS[COMP_CWORD]}"

    # take the last word that's NOT starting with -
    while [[ ( "$i" < "$COMP_CWORD" ) ]]; do
        local s="${COMP_WORDS[i]}"
        case "$s" in
          -*) ;;
          *)
            cmd="$s"
            ;;
        esac
        (( i++ ))
    done

    case "$cmd" in
      *)
          word_list="  -L -h -? -help --help -version -buildconf -formats -muxers -demuxers -devices -codecs -decoders -encoders -bsfs -protocols -filters -pix_fmts -layouts -sample_fmts -dispositions -colors -sources -sinks -hwaccels -loglevel -v -report -max_alloc -y -n -ignore_unknown -filter_threads -filter_complex_threads -stats -max_error_rate -vol -cpuflags -cpucount -hide_banner -copy_unknown -recast_media -benchmark -benchmark_all -progress -stdin -timelimit -dump -hex -vsync -frame_drop_threshold -async -adrift_threshold -copyts -start_at_zero -copytb -dts_delta_threshold -dts_error_threshold -x -abort_on -filter_complex -lavfi -filter_complex_script -auto_conversion_filters -stats_period -debug_ts -psnr -vstats -vstats_file -vstats_version -qphist -sdp_file -vaapi_device -init_hw_device -filter_hw_device -f -c -codec -pre -map_metadata -t -to -fs -ss -sseof -seek_timestamp -timestamp -metadata -program -target -apad -frames -filter -filter_script -reinit_filter -discard -disposition -map -map_channel -map_chapters -accurate_seek -isync -itsoffset -itsscale -dframes -re -readrate -shortest -bitexact -copyinkf -copypriorss -tag -q -qscale -profile -attach -dump_attachment -stream_loop -thread_queue_size -find_stream_info -bits_per_raw_sample -autorotate -autoscale -muxdelay -muxpreload -time_base -enc_time_base -bsf -fpre -max_muxing_queue_size -muxing_queue_data_threshold -d -vframes -r -fpsmax -s -aspect -vn -v -timecode -pass -vf -ab -b -dn -pix_fmt -rc_override -passlogfile -intra_matrix -inter_matrix -chroma_intra_matrix -top -vtag -fps_mode -force_fps -streamid -force_key_frames -hwaccel -hwaccel_device -hwaccel_output_format -vbsf -vpre -aframes -aq -ar -ac -an -a -af -atag -sample_fmt -channel_layout -ch_layout -guess_layout_max -absf -apre -sn -s -stag -fix_sub_duration -canvas_size -spre"
          COMPREPLY=( $(compgen -W "${word_list}" -- "${cur}") )
          ;;
    esac

}

## -o bashdefault and -o default are fallback
complete -o bashdefault -o default -F _ffmpeg ffmpeg
