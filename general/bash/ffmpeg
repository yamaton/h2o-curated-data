# Auto-generated with h2o

_ffmpeg()
{
    local i=1 cmd cur word_list
    cur="${COMP_WORDS[COMP_CWORD]}"

    # take the last word that's NOT starting with -
    while [[ ( "$i" < "$COMP_CWORD" ) ]]; do
        local s="${COMP_WORDS[i]}"
        case "$s" in
          -*) ;;
          *)
            cmd="$s"
            ;;
        esac
        (( i++ ))
    done

    case "$cmd" in
      *)
          word_list="  -L -h -? -help --help -version -formats -demuxers -muxers -devices -codecs -decoders -encoders -bsfs -protocols -filters -pix_fmts -sample_fmts -layouts -colors -sources -sinks -loglevel -v -report -hide_banner -cpuflags -f -i -y -n -stream_loop -c -codec -t -to -fs -ss -sseof -itsoffset -itsscale -timestamp -metadata -disposition -program -target -dn -dframes -frames -q -qscale -filter -filter_script -filter_threads -pre -stats -progress -stdin -debug_ts -attach -dump_attachment -noautorotate -vframes -r -s -aspect -vn -vcodec -pass -passlogfile -vf -pix_fmt -sws_flags -rc_override -ilme -psnr -vstats -vstats_file -vstats_version -top -dc -vtag -qphist -vbsf -force_key_frames -copyinkf -init_hw_device -init_hw_device -filter_hw_device -hwaccel -hwaccel_device -hwaccels -aframes -ar -aq -ac -an -acodec -sample_fmt -af -atag -absf -guess_layout_max -scodec -sn -sbsf -fix_sub_duration -canvas_size -ignore_unknown -copy_unknown -map_channel -map_metadata -map_chapters -benchmark -benchmark_all -timelimit -dump -hex -re -vsync -frame_drop_threshold -async -copyts -start_at_zero -copytb -enc_time_base -bitexact -shortest -dts_delta_threshold -muxdelay -muxpreload -streamid -bsf -tag -timecode -filter_complex -filter_complex_threads -lavfi -filter_complex_script -accurate_seek -seek_timestamp -thread_queue_size -sdp_file -discard -abort_on -xerror -max_muxing_queue_size"
          COMPREPLY=( $(compgen -W "${word_list}" -- "${cur}") )
          ;;
    esac

}

## -o bashdefault and -o default are fallback
complete -o bashdefault -o default -F _ffmpeg ffmpeg
