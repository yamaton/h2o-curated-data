# Auto-generated with h2o

_pandoc()
{
    local i=1 cmd cur word_list
    cur="${COMP_WORDS[COMP_CWORD]}"

    # take the last word that's NOT starting with -
    while [[ ( "$i" < "$COMP_CWORD" ) ]]; do
        local s="${COMP_WORDS[i]}"
        case "$s" in
          -*) ;;
          *)
            cmd="$s"
            ;;
        esac
        (( i++ ))
    done

    case "$cmd" in
      *)
          word_list="  -f -r --from --read -t -w --to --write -o --output --data-dir -d --defaults --bash-completion --verbose --quiet --fail-if-warnings --log --list-input-formats --list-output-formats --list-extensions --list-highlight-languages --list-highlight-styles -v --version -h --help --shift-heading-level-by --base-header-level --indented-code-classes --default-image-extension --file-scope -F --filter -L --lua-filter -M --metadata --metadata-file -p --preserve-tabs --tab-stop --track-changes --extract-media --abbreviations --trace -s --standalone --template -V --variable --sandbox -D --print-default-template --print-default-data-file --eol --dpi --wrap --columns --toc --table-of-contents --toc-depth --strip-comments --no-highlight --highlight-style --print-highlight-style --syntax-definition -H --include-in-header -B --include-before-body -A --include-after-body --resource-path --request-header --no-check-certificate --self-contained --embed-resources --html-q-tags --ascii --reference-links --reference-location --markdown-headings --list-tables --top-level-division -N --number-sections --number-offset --listings -i --incremental --slide-level --section-divs --email-obfuscation --id-prefix -T --title-prefix -c --css --reference-doc --epub-cover-image --epub-metadata --epub-embed-font --epub-chapter-level --epub-subdirectory --ipynb-output --pdf-engine --pdf-engine-opt -C --citeproc --bibliography --csl --citation-abbreviations --natbib --biblatex --mathjax --mathml --webtex --katex --gladtex --dump-args --ignore-args"
          COMPREPLY=( $(compgen -W "${word_list}" -- "${cur}") )
          ;;
    esac

}

## -o bashdefault and -o default are fallback
complete -o bashdefault -o default -F _pandoc pandoc
