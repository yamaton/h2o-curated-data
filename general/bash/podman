        case "$s" in
          add) _podman_add
            return
            ;;

          annotate) _podman_annotate
            return
            ;;

          create) _podman_create
            return
            ;;

          push) _podman_push
            return
            ;;

          *)  ;; 
        esac
        (( subcommand_index++ ))
    done

    word_list=" add annotate create push" 
    cur="${COMP_WORDS[COMP_CWORD]}"
    COMPREPLY=( $(compgen -W "${word_list}" -- "${cur}") )
}

