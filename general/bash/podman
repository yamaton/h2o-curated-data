          sign) _podman_sign
            return
            ;;

          tag) _podman_tag
            return
            ;;

          trust) _podman_trust
            return
            ;;

          unmount) _podman_unmount
            return
            ;;

          untag) _podman_untag
            return
            ;;

          *)  ;; 
        esac
        (( subcommand_index++ ))
    done

    word_list=" build diff history import inspect list load mount prune pull push rm save search sign tag trust unmount untag" 
    cur="${COMP_WORDS[COMP_CWORD]}"
    COMPREPLY=( $(compgen -W "${word_list}" -- "${cur}") )
}

_image_build ()
{
    local cur word_list
