ription":"Write output to a file named as the remote file"},{"names":["--remote-name-all"],"argument":"","description":"Use the remote file name for all URLs"},{"names":["-R","--remote-time"],"argument":"","description":"Set the remote file's time on the local output"},{"names":["-X","--request"],"argument":"<command>","description":"Specify request command to use"},{"names":["--request-target"],"argument":"","description":"Specify the target for this request"},{"names":["--resolve"],"argument":"<host:port:address[,address]...>","description":"Resolve the host+port to this address"},{"names":["--retry"],"argument":"<num>","description":"Retry request if transient problems occur"},{"names":["--retry-connrefused"],"argument":"","description":"Retry on connection refused (use with --retry)"},{"names":["--retry-delay"],"argument":"<seconds>","description":"Wait time between retries"},{"names":["--retry-max-time"],"argument":"<seconds>","description":"Retry only within this period"},{"names":["--sasl-authzid"],"argument":"<identity>","description":"Use this identity to act as during SASL PLAIN authentication"},{"names":["--sasl-ir"],"argument":"","description":"Enable initial response in SASL authentication"},{"names":["--service-name"],"argument":"<name>","description":"SPNEGO service name"},{"names":["-S","--show-error"],"argument":"","description":"Show error even when -s is used"},{"names":["-s","--silent"],"argument":"","description":"Silent mode"},{"names":["--socks4"],"argument":"<host[:port]>","description":"SOCKS4 proxy on given host + port"},{"names":["--socks4a"],"argument":"<host[:port]>","description":"SOCKS4a proxy on given host + port"},{"names":["--socks5"],"argument":"<host[:port]>","description":"SOCKS5 proxy on given host + port"},{"names":["--socks5-basic"],"argument":"Enable","description":"username/password auth for SOCKS5 proxies"},{"names":["--socks5-gssapi"],"argument":"Enable","description":"GSS-API auth for SOCKS5 proxies"},{"names":["--socks5-