{"name":"rsync","description":"rsync","options":[{"names":["-v","--verbose"],"argument":"","description":"increase verbosity"},{"names":["--info"],"argument":"FLAGS","description":"fine-grained informational verbosity"},{"names":["--debug"],"argument":"FLAGS","description":"fine-grained debug verbosity"},{"names":["--msgs2stderr"],"argument":"","description":"special output handling for debugging"},{"names":["-q","--quiet"],"argument":"","description":"suppress non-error messages"},{"names":["--no-motd"],"argument":"","description":"suppress daemon-mode MOTD (see caveat)"},{"names":["-c","--checksum"],"argument":"","description":"skip based on checksum, not mod-time & size"},{"names":["-a","--archive"],"argument":"","description":"archive mode; equals -rlptgoD (no -H,-A,-X)"},{"names":["--no-OPTION"],"argument":"","description":"turn off an implied OPTION (e.g. --no-D)"},{"names":["-r","--recursive"],"argument":"","description":"recurse into directories"},{"names":["-R","--relative"],"argument":"","description":"use relative path names"},{"names":["--no-implied-dirs"],"argument":"","description":"don't send implied dirs with --relative"},{"names":["-b","--backup"],"argument":"","description":"make backups (see --suffix & --backup-dir)"},{"names":["--backup-dir"],"argument":"DIR","description":"make backups into hierarchy based in DIR"},{"names":["--suffix"],"argument":"SUFFIX","description":"backup suffix (default ~ w/o --backup-dir)"},{"names":["-u","--update"],"argument":"","description":"skip files that are newer on the receiver"},{"names":["--inplace"],"argument":"","description":"update destination files in-place"},{"names":["--append"],"argument":"","description":"append data onto shorter files"},{"names":["--append-verify"],"argument":"","description":"--append w/old data in file checksum"},{"names":["-d","--dirs"],"argument":"","description":"transfer directories without recursing"},{"names":["-l","--links"],"argument":"","description":"copy symlinks as syml