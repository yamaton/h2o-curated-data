#compdef _ansible ansible

# Generated with h2o 0.1.18


function _ansible {
    local line state

    function _commands {
        local -a commands
        commands=(
        )
        _describe 'command' commands
    }
 

    _arguments -C \
        '--ask-vault-pass[ask for vault password]' \
        '--become-method[privilege escalation method to use (default=%(default)s), use ansible-doc -t become -l to list valid choices.]' \
        '--become-user[run operations as this user (default=root)]' \
        '--list-hosts[outputs a list of matching hosts; does not execute anything else]' \
        '--playbook-dir[Since this tool does not use playbooks, use this as a substitute playbook directory.This sets the relative path for many features including roles/ group_vars/ etc.]' \
        '(--private-key --key-file)'{--private-key,--key-file}'[use this file to authenticate the connection]' \
        '--scp-extra-args[specify extra arguments to pass to scp only (e.g. -l)]' \
        '--sftp-extra-args[specify extra arguments to pass to sftp only (e.g. -f, -l)]' \
        '--ssh-common-args[specify common arguments to pass to sftp/scp/ssh (e.g. ProxyCommand)]' \
        '--ssh-extra-args[specify extra arguments to pass to ssh only (e.g. -R)]' \
        '--syntax-check[perform a syntax check on the playbook, but do not execute it]' \
        '--vault-id[the vault identity to use]' \
        '--vault-password-file[vault password file]' \
        '--version[show program'\''s version number, config file location, configured module search path, module location, executable location and exit]' \
        '(-B --background)'{-B,--background}'[run asynchronously, failing after X seconds (default=N/A)]' \
        '(-C --check)'{-C,--check}'[don'\''t make any changes; instead, try to predict some of the changes that may occur]' \
        '(-D --diff)'{-D,--diff}'[when changing (small) files and templates, show the differences in those files; works great with --check]' \
        '(-K --ask-become-pass)'{-K,--ask-become-pass}'[ask for privilege escalation password]' \
        '(-M --module-path)'{-M,--module-path}'[prepend colon-separated path(s) to module library (default=~/.ansible/plugins/modules:/usr/share/ansible/plugins/modules)]' \
        '(-P --poll)'{-P,--poll}'[set the poll interval if using -B (default=15)]' \
        '(-T --timeout)'{-T,--timeout}'[override the connection timeout in seconds (default=10)]' \
        '(-a --args)'{-a,--args}'[module arguments]' \
        '(-b --become)'{-b,--become}'[run operations with become (does not imply password prompting)]' \
        '(-c --connection)'{-c,--connection}'[connection type to use (default=smart)]' \
        '(-e --extra-vars)'{-e,--extra-vars}'[set additional variables as key=value or YAML/JSON, if filename prepend with @]' \
        '(-f --forks)'{-f,--forks}'[specify number of parallel processes to use (default=5)]' \
        '(-h --help)'{-h,--help}'[show this help message and exit]' \
        '(-i --inventory --inventory-file)'{-i,--inventory,--inventory-file}'[specify inventory host path or comma separated host list. --inventory-file is deprecated]' \
        '(-k --ask-pass)'{-k,--ask-pass}'[ask for connection password]' \
        '(-l --limit)'{-l,--limit}'[further limit selected hosts to an additional pattern]' \
        '(-m --module-name)'{-m,--module-name}'[module name to execute (default=command)]' \
        '(-o --one-line)'{-o,--one-line}'[condense output]' \
        '(-t --tree)'{-t,--tree}'[log output to this directory]' \
        '(-u --user)'{-u,--user}'[connect as this user (default=None)]' \
        '(-v --verbose)'{-v,--verbose}'[verbose mode (-vvv for more, -vvvv to enable connection debugging)]' \
        '1: :_commands' \
        '*: :_files'

    case $state in
    (cmd)
        _commands
        ;;
    (subcmd)
        case $line[1] in
        esac
        ;;
     esac

}

