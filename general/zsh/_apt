#compdef _apt apt

# Auto-generated with h2o

    function _apt_list {
        _arguments \
            '--installed[Installed packages]' \
            '--upgradable[Upgradable packages]' \
            '--all-versions[Show all versions of any package]' \
            "*: :_files"

    }

    function _apt_search {
        _arguments \
            "*: :_files"

    }

    function _apt_show {
        _arguments \
            "*: :_files"

    }

    function _apt_install {
        _arguments \
            "*: :_files"

    }

