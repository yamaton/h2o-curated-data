    }
 

    _arguments -C \
        '(-0 --null)'{-0,--null}'[end each output line with NUL, not newline]' \
        '(-a --all)'{-a,--all}'[write counts for all files, not just directories]' \
        '--apparent-size[print apparent sizes, rather than disk usage; although]' \
        '(-B --block-size)'{-B,--block-size}'[scale sizes by SIZE before printing them; e.g.,]' \
