#compdef _ldconfig ldconfig

# Auto-generated with h2o


function _ldconfig {
    local line state

    function _commands {
        local -a commands
        commands=(
        )
        _describe 'command' commands
    }
 

    _arguments -C \
        {-c,--format}'[Format to use: new (default), old, or compat]' \
        '-C[Use CACHE as cache file]' \
        '-f[Use CONF as configuration file]' \
        {-i,--ignore-aux-cache}'[Ignore auxiliary cache file]' \
        '-l[Manually link individual libraries.]' \
        '-n[Only process directories specified on the command line. Don'\''t build cache.]' \
        '-N[Don'\''t build cache]' \
        {-p,--print-cache}'[Print cache]' \
        '-r[Change to and use ROOT as root directory]' \
        {-v,--verbose}'[Generate verbose messages]' \
        '-X[Don'\''t update symbolic links]' \
        {-?,--help}'[Give this help list]' \
        '--usage[Give a short usage message]' \
        {-V,--version}'[Print program version]' \
        '*: :_files'

    case $state in
    (cmd)
        _commands
        ;;
    (subcmd)
        case $line[1] in
        esac
        ;;
     esac

}

