function _mycli {
    local line state

    function _commands {
        local -a commands
        commands=(
        )
        _describe 'command' commands
    }
 

    _arguments -C \
        {-h,--host}'[Host address of the database.]' \
        {-P,--port}'[Port number to use for connection. Honors $MYSQL_TCP_PORT.]' \
        {-u,--user}'[User name to connect to the database.]' \
        {-S,--socket}'[The socket file to use for connection.]' \
        {-p,--password}'[Password to connect to the database.]' \
        '--pass[Password to connect to the database.]' \
        '--ssh-user[User name to connect to ssh server.]' \
        '--ssh-host[Host name to connect to ssh server.]' \
        '--ssh-port[Port to connect to ssh server.]' \
        '--ssh-password[Password to connect to ssh server.]' \
        '--ssh-key-filename[Private key filename (identify file) for the ssh connection.]' \
        '--ssh-config-path[Path to ssh configuration.]' \
        '--ssh-config-host[Host to connect to ssh server reading from ssh configuration.]' \
        '--ssl-ca[CA file in PEM format.]':file:_files \
        '--ssl-capath[CA directory.]' \
        '--ssl-cert[X509 cert in PEM format.]':file:_files \
        '--ssl-key[X509 key in PEM format.]':file:_files \
        '--ssl-cipher[SSL cipher to use.]' \
        '--ssl-verify-server-cert[Verify server'\''s "Common Name" in its cert against hostname used when connecting. This option is disabled by default.]' \
        {-V,--version}'[Output mycli'\''s version.]' \
        {-v,--verbose}'[Verbose output.]' \
        {-D,--database}'[Database to use.]' \
        {-d,--dsn}'[Use DSN configured into the \[alias_dsn\] section of myclirc file.]' \
        '--list-dsn[list of DSN configured into the \[alias_dsn\] section of myclirc file.]' \
        '--list-ssh-config[list ssh configurations in the ssh config (requires paramiko).]' \
        {-R,--prompt}'[Prompt format (Default: "\t \u@\h:\d> ").]' \
        {-l,--logfile}'[Log every query and its results to a file.]':file:_files \
        '--defaults-group-suffix[Read MySQL config groups with the specified suffix.]' \
        '--defaults-file[Only read MySQL options from the given file.]':file:_files \
        '--myclirc[Location of myclirc file.]':file:_files \
        '--auto-vertical-output[Automatically switch to vertical output mode if the result is wider than the terminal width.]' \
        {-t,--table}'[Display batch output in table format.]' \
        '--csv[Display batch output in CSV format.]' \
        {--warn,--no-warn}'[Warn before running a destructive query.]' \
        '--local-infile[Enable/disable LOAD DATA LOCAL INFILE.]' \
        {-g,--login-path}'[Read this path from the login file.]' \
        {-e,--execute}'[Execute command and quit.]' \
        '--init-command[SQL statement to execute after connecting.]' \
        '--charset[Character set for MySQL session.]' \
        '--password-file[File or FIFO path containing the password to connect to the db if not specified otherwise.]':file:_files \
        '--help[Show this message and exit.]' \
        '*: :_files'

    case $state in
    (cmd)
        _commands
        ;;
    (subcmd)
        case $line[1] in
        esac
        ;;
     esac

}

