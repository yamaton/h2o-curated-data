#compdef _nim nim

# Auto-generated with h2o

    function _nim_compile {
        _arguments \
            "*: :_files"

    }

    function _nim_r {
        _arguments \
            "*: :_files"

    }

    function _nim_doc {
        _arguments \
            "*: :_files"

    }


function _nim {
    local line state

    function _commands {
        local -a commands
        commands=(
            'compile:compile project with default code generator (C)'
            'r:compile to $nimcache/projname, run with \[arguments\] using backend specified by `--backend` (default: c)'
            'doc:generate the documentation for inputfile for backend specified by `--backend` (default: c)'
        )
        _describe 'command' commands
    }
 

    _arguments -C \
        ': :->cmd' \
        '*:: :->subcmd'

    case $state in
    (cmd)
        _commands
        ;;
    (subcmd)
        case $line[1] in
        (compile)
            _nim_compile
            ;;

        (r)
            _nim_r
            ;;

        (doc)
            _nim_doc
            ;;

        esac
        ;;
     esac

}

