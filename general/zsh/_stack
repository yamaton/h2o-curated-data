    function _stack_ghci {
        _arguments \
            '--pedantic[Turn on -Wall and -Werror]' \
            '--ghci-options[Additional options passed to GHCi]' \
            '--ghc-options[Additional options passed to both GHC and GHCi]' \
            '--flag[Override flags set in stack.yaml (applies to local packages and extra-deps)]' \
            '--with-ghc[Use this GHC to run GHCi]' \
            '--load[Enable/disable load modules on start-up (default: enabled)]' \
            '--no-load[Enable/disable load modules on start-up (default: enabled)]' \
            '--package[Add a package (can be specified multiple times)]' \
            '--main-is[Specify which target should contain the main module to load, such as for an executable for test suite or benchmark.]' \
            '--load-local-deps[Load all local dependencies of your targets]' \
            '--package-hiding[Enable/disable package hiding]' \
            '--no-package-hiding[Enable/disable package hiding]' \
            '--only-main[Only load and import the main module. If no main module, no modules will be loaded.]' \
            '--trace[Enable profiling in libraries, executables, etc. for all expressions and generate a backtrace on exception]' \
            '--profile[profiling in libraries, executables, etc. for all expressions and generate a profiling report in tests or benchmarks]' \
            '--no-strip[Disable DWARF debugging symbol stripping in libraries, executables, etc. for all expressions, producing larger executables but allowing the use of standard debuggers/profiling tools/other utilities that use debugging symbols.]' \
            '--test[Enable/disable testing the package(s) in this directory/configuration (default: disabled)]' \
            '--no-test[Enable/disable testing the package(s) in this directory/configuration (default: disabled)]' \
            '--bench[Enable/disable benchmarking the package(s) in this directory/configuration (default: disabled)]' \
            '--no-bench[Enable/disable benchmarking the package(s) in this directory/configuration (default: disabled)]' \
            '--setup-info-yaml[Alternate URL or relative / absolute path for stack dependencies]' \
            '--snapshot-location-base[The base location of LTS/Nightly snapshots]' \
            '--help[Show this help text]' \
            "*: :_files"

    }

    function _stack_repl {
        _arguments \
            '--pedantic[Turn on -Wall and -Werror]' \
            '--ghci-options[Additional options passed to GHCi]' \
            '--ghc-options[Additional options passed to both GHC and GHCi]' \
            '--flag[Override flags set in stack.yaml (applies to local packages and extra-deps)]' \
            '--with-ghc[Use this GHC to run GHCi]' \
            '--load[Enable/disable load modules on start-up (default: enabled)]' \
            '--no-load[Enable/disable load modules on start-up (default: enabled)]' \
            '--package[Add a package (can be specified multiple times)]' \
            '--main-is[Specify which target should contain the main module to load, such as for an executable for test suite or benchmark.]' \
            '--load-local-deps[Load all local dependencies of your targets]' \
            '--package-hiding[Enable/disable package hiding]' \
            '--no-package-hiding[Enable/disable package hiding]' \
            '--only-main[Only load and import the main module. If no main module, no modules will be loaded.]' \
            '--trace[Enable profiling in libraries, executables, etc. for all expressions and generate a backtrace on exception]' \
            '--profile[profiling in libraries, executables, etc. for all expressions and generate a profiling report in tests or benchmarks]' \
            '--no-strip[Disable DWARF debugging symbol stripping in libraries, executables, etc. for all expressions, producing larger executables but allowing the use of standard debuggers/profiling tools/other utilities that use debugging symbols.]' \
            '--test[Enable/disable testing the package(s) in this directory/configuration (default: disabled)]' \
            '--no-test[Enable/disable testing the package(s) in this directory/configuration (default: disabled)]' \
            '--bench[Enable/disable benchmarking the package(s) in this directory/configuration (default: disabled)]' \
            '--no-bench[Enable/disable benchmarking the package(s) in this directory/configuration (default: disabled)]' \
            '--setup-info-yaml[Alternate URL or relative / absolute path for stack dependencies]' \
            '--snapshot-location-base[The base location of LTS/Nightly snapshots]' \
            '--help[Show this help text]' \
            "*: :_files"

    }

    function _stack_runghc {
        _arguments \
            '--ghc-package-path[Enable/disable setting the GHC_PACKAGE_PATH variable for the subprocess (default: enabled)]' \
            '--no-ghc-package-path[Enable/disable setting the GHC_PACKAGE_PATH variable for the subprocess (default: enabled)]' \
            '--stack-exe[Enable/disable setting the STACK_EXE environment variable to the path for the stack executable (default: enabled)]' \
            '--no-stack-exe[Enable/disable setting the STACK_EXE environment variable to the path for the stack executable (default: enabled)]' \
            '--package[Add a package (can be specified multiple times)]' \
            '--rts-options[Explicit RTS options to pass to application]' \
            '--cwd[Sets the working directory before executing]':file:_files \
            '--setup-info-yaml[Alternate URL or relative / absolute path for stack dependencies]' \
            '--snapshot-location-base[The base location of LTS/Nightly snapshots]' \
            '--help[Show this help text]' \
            "*: :_files"

    }

    function _stack_runhaskell {
        _arguments \
            '--ghc-package-path[Enable/disable setting the GHC_PACKAGE_PATH variable for the subprocess (default: enabled)]' \
            '--no-ghc-package-path[Enable/disable setting the GHC_PACKAGE_PATH variable for the subprocess (default: enabled)]' \
            '--stack-exe[Enable/disable setting the STACK_EXE environment variable to the path for the stack executable (default: enabled)]' \
            '--no-stack-exe[Enable/disable setting the STACK_EXE environment variable to the path for the stack executable (default: enabled)]' \
            '--package[Add a package (can be specified multiple times)]' \
            '--rts-options[Explicit RTS options to pass to application]' \
            '--cwd[Sets the working directory before executing]':file:_files \
            '--setup-info-yaml[Alternate URL or relative / absolute path for stack dependencies]' \
            '--snapshot-location-base[The base location of LTS/Nightly snapshots]' \
            '--help[Show this help text]' \
            "*: :_files"

    }

    function _stack_script {
        _arguments \
            '--package[Add a package (can be specified multiple times)]' \
            '--compile[Compile the script without optimization and run the executable]' \
            '--optimize[Compile the script with optimization and run the executable]' \
            '--ghc-options[Additional options passed to GHC]' \
            '--extra-dep[Extra dependencies to be added to the snapshot]' \
            '--no-run[Don'\''t run, just compile.]' \
            '--setup-info-yaml[Alternate URL or relative / absolute path for stack dependencies]' \
            '--snapshot-location-base[The base location of LTS/Nightly snapshots]' \
            '--help[Show this help text]' \
            "*: :_files"

    }

    function _stack_eval {
        _arguments \
            '--ghc-package-path[Enable/disable setting the GHC_PACKAGE_PATH variable for the subprocess (default: enabled)]' \
            '--no-ghc-package-path[Enable/disable setting the GHC_PACKAGE_PATH variable for the subprocess (default: enabled)]' \
            '--stack-exe[Enable/disable setting the STACK_EXE environment variable to the path for the stack executable (default: enabled)]' \
            '--no-stack-exe[Enable/disable setting the STACK_EXE environment variable to the path for the stack executable (default: enabled)]' \
            '--package[Add a package (can be specified multiple times)]' \
            '--rts-options[Explicit RTS options to pass to application]' \
            '--cwd[Sets the working directory before executing]':file:_files \
            '--setup-info-yaml[Alternate URL or relative / absolute path for stack dependencies]' \
            '--snapshot-location-base[The base location of LTS/Nightly snapshots]' \
            '--help[Show this help text]' \
            "*: :_files"

    }

    function _stack_clean {
        _arguments \
            '--full[Delete the project’s stack working directories (.stack-work by default).]' \
            '--setup-info-yaml[Alternate URL or relative / absolute path for stack dependencies]' \
            '--snapshot-location-base[The base location of LTS/Nightly snapshots]' \
            '--help[Show this help text]' \
            "*: :_files"

    }

    function _stack_purge {
        _arguments \
            '--setup-info-yaml[Alternate URL or relative / absolute path for stack dependencies]' \
            '--snapshot-location-base[The base location of LTS/Nightly snapshots]' \
            '--help[Show this help text]' \
            "*: :_files"

    }

    function _stack_query {
        _arguments \
            '--setup-info-yaml[Alternate URL or relative / absolute path for stack dependencies]' \
            '--snapshot-location-base[The base location of LTS/Nightly snapshots]' \
            '--help[Show this help text]' \
            "*: :_files"

    }

    function _stack_list {
        _arguments \
            '--setup-info-yaml[Alternate URL or relative / absolute path for stack dependencies]' \
            '--snapshot-location-base[The base location of LTS/Nightly snapshots]' \
            '--help[Show this help text]' \
            "*: :_files"

    }

    function _stack_ide {
        _arguments \
            '--setup-info-yaml[Alternate URL or relative / absolute path for stack dependencies]' \
            '--snapshot-location-base[The base location of LTS/Nightly snapshots]' \
            '--help[Show this help text]' \
            "*: :_files"

    }

    function _stack_docker {
        _arguments \
            '--setup-info-yaml[Alternate URL or relative / absolute path for stack dependencies]' \
            '--snapshot-location-base[The base location of LTS/Nightly snapshots]' \
            '--help[Show this help text]' \
            "*: :_files"

    }

    function _stack_config {
        _arguments \
            '--setup-info-yaml[Alternate URL or relative / absolute path for stack dependencies]' \
            '--snapshot-location-base[The base location of LTS/Nightly snapshots]' \
            '--help[Show this help text]' \
            "*: :_files"

    }

    function _stack_hpc {
        _arguments \
            '--setup-info-yaml[Alternate URL or relative / absolute path for stack dependencies]' \
            '--snapshot-location-base[The base location of LTS/Nightly snapshots]' \
            '--help[Show this help text]' \
            "*: :_files"

    }


function _stack {
    local line state

    function _commands {
        local -a commands
        commands=(
            'build:Build the package(s) in this directory/configuration'
            'install:Shortcut for '\''build --copy-bins'\'''
            'uninstall:Show how to uninstall Stack. This command does not'
            'test:Shortcut for '\''build --test'\'''
            'bench:Shortcut for '\''build --bench'\'''
            'haddock:Shortcut for '\''build --haddock'\'''
            'new:Create a new project from a template. Run `stack'
            'templates:Show how to find templates available for `stack new'\''.'
            'init:Create stack project config from cabal or hpack'
            'setup:Get the appropriate GHC for your project'
            'path:Print out handy path information'
            'ls:List command. (Supports snapshots, dependencies,'
            'unpack:Unpack one or more packages locally'
            'update:Update the package index'
            'upgrade:Upgrade to the latest stack'
            'upload:Upload a package to Hackage'
            'sdist:Create source distribution tarballs'
            'dot:Visualize your project'\''s dependency graph using'
            'ghc:Run ghc'
            'hoogle:Run hoogle, the Haskell API search engine. Use the'
            'exec:Execute a command. If the command is absent, the'
            'run:Build and run an executable. Defaults to the first'
            'ghci:Run ghci in the context of package(s) (experimental)'
            'repl:Run ghci in the context of package(s) (experimental)'
            'runghc:Run runghc'
            'runhaskell:Run runghc (alias for '\''runghc'\'')'
            'script:Run a Stack Script'
            'eval:Evaluate some haskell code inline. Shortcut for'
            'clean:Delete build artefacts for the project packages.'
            'purge:Delete the project stack working directories'
            'query:Query general build information (experimental)'
            'list:List package id'\''s in snapshot (experimental)'
            'ide:IDE-specific commands'
            'docker:Subcommands specific to Docker use'
            'config:Subcommands for accessing and modifying configuration'
            'hpc:Subcommands specific to Haskell Program Coverage'
        )
        _describe 'command' commands
    }
 

    _arguments -C \
        '--help[Show this help text]' \
        '--version[Show version]' \
        '--numeric-version[Show only version number]' \
        '--hpack-numeric-version[Show only hpack'\''s version number]' \
        '--docker[Run '\''stack --docker-help'\'' for details]' \
        '--nix[Run '\''stack --nix-help'\'' for details]' \
        '--verbosity[Verbosity: silent, error, warn, info, debug]' \
        {-v,--verbose}'[Enable verbose mode: verbosity level "debug"]' \
        '--silent[Enable silent mode: verbosity level "silent"]' \
        '--time-in-log[Enable/disable inclusion of timings in logs, for the purposes of using diff with logs (default: enabled)]' \
        '--no-time-in-log[Enable/disable inclusion of timings in logs, for the purposes of using diff with logs (default: enabled)]' \
        '--rsl-in-log[Enable/disable inclusion of raw snapshot layer (rsl) in logs (default: disabled)]' \
        '--no-rsl-in-log[Enable/disable inclusion of raw snapshot layer (rsl) in logs (default: disabled)]' \
        '--stack-root[Absolute path to the global stack root directory (Overrides any STACK_ROOT environment variable)]' \
        '--work-dir[Relative path of work directory (Overrides any STACK_WORK environment variable, default is '\''.stack-work'\'')]':file:_files \
        '--system-ghc[Enable/disable using the system installed GHC (on the PATH) if it is available and its version matches. Disabled by default.]' \
        '--no-system-ghc[Enable/disable using the system installed GHC (on the PATH) if it is available and its version matches. Disabled by default.]' \
        '--install-ghc[Enable/disable downloading and installing GHC if necessary (can be done manually with stack setup) (default: enabled)]' \
        '--no-install-ghc[Enable/disable downloading and installing GHC if necessary (can be done manually with stack setup) (default: enabled)]' \
        '--arch[System architecture, e.g. i386, x86_64]' \
        '--ghc-variant[Specialized GHC variant, e.g. int-native or integersimple (incompatible with --system-ghc)]' \
        '--ghc-build[Specialized GHC build, e.g. '\''gmp4'\'' or '\''standard'\'' (usually auto-detected)]' \
        {-j,--jobs}'[Number of concurrent jobs to run]' \
        '--extra-include-dirs[Extra directories to check for C header files]':file:_files \
        '--extra-lib-dirs[Extra directories to check for libraries]':file:_files \
        '--custom-preprocessor-extensions[Extensions used for custom preprocessors]' \
        '--with-gcc[Use gcc found at PATH-TO-GCC]':file:_files \
        '--with-hpack[Use HPACK executable (overrides bundled Hpack)]' \
        '--skip-ghc-check[Enable/disable skipping the GHC version and architecture check (default: disabled)]' \
        '--no-skip-ghc-check[Enable/disable skipping the GHC version and architecture check (default: disabled)]' \
        '--skip-msys[Enable/disable skipping the local MSYS installation (Windows only) (default: disabled)]' \
        '--no-skip-msys[Enable/disable skipping the local MSYS installation (Windows only) (default: disabled)]' \
        '--local-bin-path[Install binaries to DIR]':file:_files \
        '--setup-info-yaml[Alternate URL or relative / absolute path for stack dependencies]' \
        '--modify-code-page[Enable/disable setting the codepage to support UTF-8 (Windows only) (default: enabled)]' \
        '--no-modify-code-page[Enable/disable setting the codepage to support UTF-8 (Windows only) (default: enabled)]' \
        '--allow-different-user[Enable/disable permission for users other than the owner of the stack root directory to use a stack installation (POSIX only) (default: true inside Docker, otherwise false)]' \
        '--no-allow-different-user[Enable/disable permission for users other than the owner of the stack root directory to use a stack installation (POSIX only) (default: true inside Docker, otherwise false)]' \
        '--dump-logs[Enable/disable dump the build output logs for local packages to the console (default: dump warning logs)]' \
        '--no-dump-logs[Enable/disable dump the build output logs for local packages to the console (default: dump warning logs)]' \
        {--color,--colour}'[Specify when to use color in output; WHEN is '\''always'\'', '\''never'\'', or '\''auto'\''. On Windows versions before Windows 10, for terminals that do not support color codes, the default is '\''never'\''; color may work on terminals that support color codes]' \
        '--snapshot-location-base[The base location of LTS/Nightly snapshots]' \
        '--script-no-run-compile[Enable/disable the use of options `--no-run --compile` with `stack script` (default: disabled)]' \
        '--no-script-no-run-compile[Enable/disable the use of options `--no-run --compile` with `stack script` (default: disabled)]' \
        '--resolver[Override resolver in project file]' \
        '--compiler[Use the specified compiler]' \
        '--terminal[Enable/disable overriding terminal detection in the case of running in a false terminal]' \
        '--no-terminal[Enable/disable overriding terminal detection in the case of running in a false terminal]' \
        {--stack-colors,--stack-colours}'[Specify stack'\''s output styles; STYLES is a colon-delimited sequence of key=value, where '\''key'\'' is a style name and '\''value'\'' is a semicolon-delimited list of '\''ANSI'\'' SGR (Select Graphic Rendition) control codes (in decimal). Use '\''stack ls stack-colors --basic'\'' to see the current sequence. In shells where a semicolon is a command separator, enclose STYLES in quotes.]' \
        '--terminal-width[Specify the width of the terminal, used for pretty-print messages]' \
        '--stack-yaml[Override project stack.yaml file (overrides any STACK_YAML environment variable)]' \
        '--lock-file[Specify how to interact with lock files. Default: read/write. If resolver is overridden: read-only]' \
        '--full[query Query general build information (experimental)]' \
        ': :->cmd' \
        '*:: :->subcmd'

    case $state in
    (cmd)
        _commands
        ;;
    (subcmd)
        case $line[1] in
        (build)
            _stack_build
            ;;

        (install)
            _stack_install
            ;;

        (uninstall)
            _stack_uninstall
            ;;

        (test)
            _stack_test
            ;;

        (bench)
            _stack_bench
            ;;

        (haddock)
            _stack_haddock
            ;;

        (new)
            _stack_new
            ;;

        (templates)
            _stack_templates
            ;;

        (init)
            _stack_init
            ;;

        (setup)
            _stack_setup
            ;;

        (path)
            _stack_path
            ;;

        (ls)
            _stack_ls
            ;;

        (unpack)
            _stack_unpack
            ;;

        (update)
            _stack_update
            ;;

        (upgrade)
            _stack_upgrade
            ;;

        (upload)
            _stack_upload
            ;;

        (sdist)
            _stack_sdist
            ;;

        (dot)
            _stack_dot
            ;;

        (ghc)
            _stack_ghc
            ;;

        (hoogle)
            _stack_hoogle
            ;;

        (exec)
            _stack_exec
            ;;

        (run)
            _stack_run
            ;;

        (ghci)
            _stack_ghci
            ;;

        (repl)
            _stack_repl
            ;;

        (runghc)
            _stack_runghc
            ;;

        (runhaskell)
            _stack_runhaskell
            ;;

        (script)
            _stack_script
            ;;

        (eval)
            _stack_eval
            ;;

        (clean)
            _stack_clean
            ;;

        (purge)
            _stack_purge
            ;;

        (query)
            _stack_query
            ;;

        (list)
            _stack_list
            ;;

        (ide)
            _stack_ide
            ;;

        (docker)
            _stack_docker
            ;;

        (config)
            _stack_config
            ;;

        (hpc)
            _stack_hpc
            ;;

        esac
        ;;
     esac

}

