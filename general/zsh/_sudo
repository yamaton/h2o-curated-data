    function _commands {
        local -a commands
        commands=(
        )
        _describe 'command' commands
    }
 

    _arguments -C \
        '(-A --askpass)'{-A,--askpass}'[use a helper program for password prompting]' \
        '(-b --background)'{-b,--background}'[run command in the background]' \
        '(-B --bell)'{-B,--bell}'[ring bell when prompting]' \
