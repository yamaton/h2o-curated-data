#compdef _wg-quick wg-quick

# Auto-generated with h2o

    function _wg-quick_up {
        _arguments \
            "*: :_files"

    }

    function _wg-quick_down {
        _arguments \
            "*: :_files"

    }

    function _wg-quick_save {
        _arguments \
            "*: :_files"

    }

    function _wg-quick_strip {
        _arguments \
            "*: :_files"

    }


function _wg-quick {
    local line state

    function _commands {
        local -a commands
        commands=(
            'up:add and set up an interface'
            'down:tear down and remove an interface'
            'save:saves the configuration of an existing interface without bringing the interface down'
            'strip:output a configuration file with all wg-quick(8)-specific options removed, suitable for use with wg(8)'
        )
        _describe 'command' commands
    }
 

    _arguments -C \
        ': :->cmd' \
        '*:: :->subcmd'

    case $state in
    (cmd)
        _commands
        ;;
    (subcmd)
        case $line[1] in
        (up)
            _wg-quick_up
            ;;

        (down)
            _wg-quick_down
            ;;

        (save)
            _wg-quick_save
            ;;

        (strip)
            _wg-quick_strip
            ;;

        esac
        ;;
     esac

}

